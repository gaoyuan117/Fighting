<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="vertical"
      android:layout_alignParentTop="true"
      android:layout_alignParentLeft="true"
      android:layout_alignParentStart="true">

    <include layout="@layout/titlebar_common"/>

    <RelativeLayout
        android:id="@+id/rlayout_my_order_click"
        android:layout_width="fill_parent"
        android:layout_height="50dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/common_write_btn_bg_selector">

      <TextView
          style="@style/Btn_Wrap_Black_Text_Title3"
          android:layout_centerVertical="true"
          android:layout_marginLeft="10dip"
          android:drawableLeft="@drawable/personal_all_order"
          android:drawablePadding="12dip"
          android:text="全部订单"/>

      <TextView
          style="@style/Text_Wrap_Gray_Text_Sm"
          android:layout_alignParentRight="true"
          android:layout_centerVertical="true"
          android:layout_marginRight="10dip"
          android:drawablePadding="8dip"
          android:drawableRight="@drawable/common_right_arrow"
          android:text=""/>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/gray_white_tra"/>

    <RelativeLayout
        android:id="@+id/rlayout_my_favorite_product"
        android:layout_width="fill_parent"
        android:layout_height="50dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/common_write_btn_bg_selector"
        android:visibility="visible">

      <TextView
          style="@style/Btn_Wrap_Black_Text_Title3"
          android:layout_centerVertical="true"
          android:layout_marginLeft="10dip"
          android:drawableLeft="@drawable/ic_view_history"
          android:drawablePadding="12dip"
          android:text="收藏的商品"
          />

      <TextView
          style="@style/Text_Wrap_Gray_Text_Sm"
          android:layout_alignParentRight="true"
          android:layout_centerVertical="true"
          android:layout_marginRight="10dip"
          android:drawablePadding="8dip"
          android:drawableRight="@drawable/common_right_arrow"/>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/gray_white_tra"/>

    <RelativeLayout
        android:id="@+id/rlayout_my_favorite_shop"
        android:layout_width="fill_parent"
        android:layout_height="50dip"
        android:background="@drawable/common_write_btn_bg_selector">

      <TextView
          style="@style/Btn_Wrap_Black_Text_Title3"
          android:layout_centerVertical="true"
          android:layout_marginLeft="10dip"
          android:drawableLeft="@drawable/ic_collect_shop"
          android:drawablePadding="12dip"
          android:text="收藏的店铺"/>

      <TextView
          style="@style/Text_Wrap_Gray_Text_Sm"
          android:layout_alignParentRight="true"
          android:layout_centerVertical="true"
          android:layout_marginRight="10dip"
          android:drawablePadding="8dip"
          android:drawableRight="@drawable/common_right_arrow"
          android:text=""/>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/gray_white_tra"/>

    <RelativeLayout
        android:id="@+id/rlayout_my_footprint"
        android:layout_width="fill_parent"
        android:layout_height="50dip"
        android:background="@drawable/common_write_btn_bg_selector">

      <TextView
          style="@style/Btn_Wrap_Black_Text_Title3"
          android:layout_centerVertical="true"
          android:layout_marginLeft="10dip"
          android:drawableLeft="@drawable/ic_collector_product"
          android:drawablePadding="12dip"
          android:text="浏览记录"/>

      <TextView
          style="@style/Text_Wrap_Gray_Text_Sm"
          android:layout_alignParentRight="true"
          android:layout_centerVertical="true"
          android:layout_marginRight="10dip"
          android:drawablePadding="8dip"
          android:drawableRight="@drawable/common_right_arrow"
          android:text=""/>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/gray_white_tra"/>

    <RelativeLayout
        android:id="@+id/rlayout_my_merchant_click"
        android:layout_width="fill_parent"
        android:layout_height="50dip"
        android:background="@drawable/common_write_btn_bg_selector">

      <TextView
          style="@style/Btn_Wrap_Black_Text_Title3"
          android:layout_centerVertical="true"
          android:layout_marginLeft="10dip"
          android:drawableLeft="@drawable/personal_edit_help"
          android:drawablePadding="12dip"
          android:text="帮助与反馈"/>

      <TextView
          style="@style/Text_Wrap_Gray_Text_Sm"
          android:layout_alignParentRight="true"
          android:layout_centerVertical="true"
          android:layout_marginRight="10dip"
          android:drawablePadding="8dip"
          android:drawableRight="@drawable/common_right_arrow"
          android:text=""/>
    </RelativeLayout>
  </LinearLayout>


</RelativeLayout>